<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="Chrome" t-inherit="point_of_sale.Chrome" t-inherit-mode="extension" owl="1">
        <xpath expr="//img[hasclass('pos-logo')]" position="after">
            <CashMoveButton  />
        </xpath>

        <xpath expr="//NotificationSound" position="before">
            <HbPosCashNotification t-if="state.notification.isShown" message="state.notification.message" duration="state.notification.duration" t-transition="fade"/>
        </xpath>
    </t>

</templates>
